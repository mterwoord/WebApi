{
    "dependencies": {
        "Microsoft.AspNetCore.Mvc": "1.1.0-alpha1-22299",
        "Microsoft.AspNetCore.OData": {
            "target": "project"
        },
        "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0-alpha1-22299",
        "Microsoft.AspNetCore.Server.Kestrel": "1.1.0-alpha1-22299",
        "Microsoft.Extensions.Logging": "1.1.0-alpha1-22299",
        "Microsoft.Extensions.Logging.Console": "1.1.0-alpha1-22299"  },

    "tools": {
    },

  "frameworks": {
    "net452": { }
  },

  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true
  },

  "publishOptions": {
    "include": [
      "wwwroot",
      "Views",
      "appsettings.json",
      "web.config"
    ]
  },

  "scripts": {
    "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
  }
}
